struct Whatever {
    a i32
    b i32
    c *i32
}

func main() {
    // a is 3, b is 4, c is 5 
    a, b, c := 3, 4, 5;
    // Now a is 4 and b is 3!
    a, b = b, a;
    if a != 4 || b != 3 {
        @print("bad interchange", a, b);
    }

    w, array, array2 := @Whatever{}, [1]i32{1}, [1]i32{1}
    w.a, w.b, w.c, array[0], array2 = 1, 2, &3, 4, [1]i32{4};
    if w.a != 1 @print("bad");
    if w.b != 2 @print("bad2");
    if *w.c != 3 @print("bad3");
    if array[0] != 4 @print("bad4");
    if array2[0] != 4 @print("bad5");
}