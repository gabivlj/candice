struct Point {
    x i32
    y i32
}

func createPoint(x i32, y i32) Point {
    thePoint := @Point{}
    thePoint.x = x;
    thePoint.y = y;
    return thePoint;
}

func changePoint(point *Point, x i32, y i32) {
    point.x = x;
    point.y = y;
    return;
}

aPoint := createPoint(3, 4)
@println(aPoint.x) // 3
@println(aPoint.y) // 4
changePoint(&aPoint, 5, 5)
@println(aPoint.x) // 5
@println(aPoint.y) // 5
